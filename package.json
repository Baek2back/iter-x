{
	"name": "iter-x",
	"version": "0.0.1",
	"sideEffects": false,
	"workspaces": ["docs"],
	"devDependencies": {
		"@biomejs/biome": "1.8.2",
		"@commitlint/cli": "19.3.0",
		"@commitlint/config-conventional": "19.2.2",
		"@microsoft/api-extractor": "7.47.0",
		"@release-it/conventional-changelog": "8.0.1",
		"@tsconfig/bun": "1.0.6",
		"@types/bun": "latest",
		"commitizen": "4.3.0",
		"cz-conventional-changelog": "3.3.0",
		"lefthook": "1.6.18",
		"release-it": "^17.4.1",
		"tsup": "8.1.0",
		"typescript": "5.5.2",
		"vitest": "1.6.0"
	},
	"peerDependencies": {
		"typescript": "^5.5.2"
	},
	"packageManager": "bun@1.1.17",
	"scripts": {
