{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "start": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:watch": {
      "outputs": []
    },
    "coverage": {
      "outputs": []
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
